{
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 8,
        "ecmaFeatures": {
            "experimentalObjectRestSpread": false,
            "jsx": true,
            "classes": true,
            "templateStrings": true,
            "modules": true
        },
        "sourceType": "module"
    },
    "extends": "airbnb",
    "env": {
        "browser": true,
        "es6": true,
        "node": true
    },
    "plugins": [
        "react",
        "jsx-a11y",
        "import"
    ],
    "globals": {
        "fetch": true,
        "url": true
    },
    "rules": {
        "react/destructuring-assignment": 0,
        "implicit-arrow-linebreak": ["warn", "beside"],
        "react/prop-types": 0
    }
}